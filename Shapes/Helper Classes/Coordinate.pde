public class Coordinate
{
  public int xCoordinate;
  public int yCoordinate;
  
  public Coordinate(int xCoordinate, int yCoordinate)
  {
    this.yCoordinate = yCoordinate;
    this.xCoordinate = xCoordinate;
  }
  
  public Coordinate()
  {
    this.yCoordinate = 0;
    this.xCoordinate = 0;
  }
}
